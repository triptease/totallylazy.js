        "madge": "^5.0.1",
        "mocha": "^9.2.2",
        "node-fetch": "^3.2.10",
        "prettier": "^3.6.2",
        "typescript": "^4.8.4",
        "wikidata-sdk": "^7.14.4",
        "xmldom": "^0.6.0",
        "xpath": "^0.0.32"
    },
    "optionalDependencies": {
        "@types/node": "^13.13.52",
        "full-icu": "^1.5.0",
        "http-shutdown": "^1.2.2"
    }
}
